unix_mkipk:
	@ if [ -e /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/data ]; then echo "ERROR: /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/data exists!"; exit 1; fi; if [ -e /home/shaoyang/mini2440/mypic/ipkg/qpe-mypic__arm.ipk ]; then echo "Package qpe-mypic__arm.ipk already exists!"; exit 0; fi; IPK_GROUP_PROJECTS="/home/shaoyang/mini2440/mypic"; for i in $${IPK_GROUP_PROJECTS};do make -C $$i -f Makefile unix_mkipk_deps INSTALL_ROOT=/home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/data/opt/Qtopia || exit $$?; done; make_ipk_file() { cd /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/data; if [ ! -z "$$1" ]; then lang="$$1"; suffix="_i18n_$$lang"; deps="qpe-mypic"; if [ "`echo qpe-mypic | cut -c -4`" = "qpe-" -a "qpe-mypic" != "qpe-taskbar" ]; then deps="$$deps qpe-taskbar_i18n_$$lang"; fi; path="opt/Qtopia/i18n/$$lang"; if [ ! -d "/home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/data/$$path" ]; then return; fi; else suffix=; deps=`cat /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/control/DEPS`; deps=`echo $$deps | sort -u`; path="opt/Qtopia"; fi; echo "Package: qpe-mypic$$suffix" > /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/control/control; echo "Maintainer: Unspecified" >> /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/control/control; echo "License: GPL" >> /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/control/control; echo "Version: " >> /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/control/control; echo "Arch: arm" >> /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/control/control; echo "Architecture: arm" >> /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/control/control; echo "Description: No description written for this package" >> /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/control/control; echo -n "Depends: " >> /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/control/control; echo $$deps >> /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/control/control; echo -n "Files:" >> /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/control/control; find $$path -type f -o -type l -printf "/%P " >> /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/control/control; if [ ! -x "`which mkfs.jffs2`" ]; then echo -e "\nInstalled-Size: $$(du -h -s $$path | awk '{print $$1}')" >> /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/control/control; else echo -e "\nInstalled-Size: $$(mkfs.jffs2 -r $$path | wc -c)" >> /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/control/control; fi; echo >> /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/control/control; for i in preinst prerm postinst postrm;do export FILE_LOC=/home/shaoyang/mini2440/mypic/qpe-mypic.$$i; if [ -e $$FILE_LOC ]; then install -c $$FILE_LOC /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/$$i; fi; done; cd /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic; echo "2.0" > debian-binary; cd /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/control; tar --remove-files -cz --owner=0 --group=0 -f /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/control.tar.gz ./control; cd /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/data; tar --remove-files -cz --owner=0 --group=0 -f /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/data.tar.gz $$path; if [ ! -e /home/shaoyang/mini2440/mypic/ipkg ];then mkdir -p /home/shaoyang/mini2440/mypic/ipkg;fi; cd /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic; tar --remove-files -cz --owner=0 --group=0 -f /home/shaoyang/mini2440/mypic/ipkg/qpe-mypic__arm$$suffix.ipk ./debian-binary ./control.tar.gz ./data.tar.gz `for i in preinst prerm postinst postrm;do if [ -e $$i ]; then echo $$i; fi; done`; }; LANGUAGES="en_US"; for lang in $${LANGUAGES}; do make_ipk_file $$lang; done; [ -d /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/data/opt/Qtopia/i18n ] && rm -r /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic/data/opt/Qtopia/i18n; make_ipk_file; rm -rf /home/shaoyang/mini2440/mypic/.packaging-working-dir/ipk/qpe-mypic

